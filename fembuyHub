local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Fembuy Hub",
   Icon = 93059603763570,
   LoadingTitle = "Loading Hub",
   LoadingSubtitle = "by Manuel",
   Theme = "Default",
   ToggleUIKeybind = "K",
   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Fembuy Hub"
   },

   Discord = {
      Enabled = false,
      Invite = "noinvitelink",
      RememberJoins = true
   },

   KeySystem = true,
   KeySettings = {
      Title = "Fembuy Hub",
      Subtitle = "Enter the key to proceed",
      Note = "Ask Manuel",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"ilovefemboys"}
   }
})

Rayfield:Notify({
   Title = "Successful Execution",
   Content = "Fembuy Hub has loaded correctly.",
   Duration = 6.5,
   Image = 4483362458,
})

-- Home Tab (different icon)
local HomeTab = Window:CreateTab("Home", 6023426915) -- example icon ID
local HomeSection = HomeTab:CreateSection("Main")

HomeTab:CreateButton({
   Name = "Infinite Yield",
   Callback = function()
      loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/Infinite%20Yield.txt"))()
   end,
})

-- Player Tab (different icon)
local PlayerTab = Window:CreateTab("Player", 6023426940) -- example icon ID
local PlayerSection = PlayerTab:CreateSection("Player Scripts")

PlayerTab:CreateButton({
   Name = "Invisible Script",
   Callback = function()
      loadstring(game:HttpGet('https://pastebin.com/raw/3Rnd9rHf'))()
   end,
})

PlayerTab:CreateSlider({
   Name = "Speed Slider",
   Range = {0, 100},
   Increment = 1,
   Suffix = "Speed",
   CurrentValue = 16, -- Default Roblox WalkSpeed
   Flag = "Slider1",
   Callback = function(Value)
      local player = game.Players.LocalPlayer
      if player and player.Character and player.Character:FindFirstChild("Humanoid") then
         player.Character.Humanoid.WalkSpeed = Value
      end
   end,
})

-- TSB Tab (different icon)
local TSBTab = Window:CreateTab("TSB", 6023426965) -- example icon ID
local TSBSection = TSBTab:CreateSection("TSB Scripts")

TSBTab:CreateButton({
   Name = "Phantasm Hub",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/ATrainz/Phantasm/main/Games/TSB.lua"))()
   end,
})

TSBTab:CreateButton({
   Name = "Another TSB Script",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/ATrainz/Phantasm/main/Games/TSB.lua"))()
   end,
})

