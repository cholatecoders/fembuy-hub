local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Fembuy Hub",
   Icon = 93059603763570,
   LoadingTitle = "Loading Hub",
   LoadingSubtitle = "by Manuel",
   Theme = "Default",
   ToggleUIKeybind = "K",
   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Fembuy Hub"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink",
      RememberJoins = true
   },
   KeySystem = true,
   KeySettings = {
      Title = "Fembuy Hub",
      Subtitle = "Enter the key to proceed",
      Note = "Ask Manuel",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"ilovefemboys"}
   }
})

Rayfield:Notify({
   Title = "Successful Execution",
   Content = "Fembuy Hub has loaded correctly.",
   Duration = 6.5,
   Image = 4483362458,
})

-- Toggle system for button scripts
local toggles = {
   ["Infinite Yield"] = false,
   ["Invisible Script"] = false,
   ["Phantasm Hub"] = false,
   ["Another TSB Script"] = false,
   ["Garden Script"] = false
}

-- Home Tab
local HomeTab = Window:CreateTab("Home", 17205450457)
HomeTab:CreateButton({
   Name = "Toggle Infinite Yield",
   Callback = function()
      toggles["Infinite Yield"] = not toggles["Infinite Yield"]
      if toggles["Infinite Yield"] then
         getgenv().infYield = loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/Infinite%20Yield.txt"))()
      else
         -- No official unload function for Infinite Yield
         Rayfield:Notify({
            Title = "Toggle Off",
            Content = "Please rejoin to fully disable Infinite Yield.",
            Duration = 5
         })
      end
   end,
})

HomeTab:CreateButton({
   Name = "Teleport Everyone To Me",
   Callback = function()
      local localPlayer = game.Players.LocalPlayer
      local root = localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart")
      if root then
         for _, player in pairs(game.Players:GetPlayers()) do
            if player ~= localPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
               player.Character.HumanoidRootPart.CFrame = root.CFrame + Vector3.new(0, 5, 0)
            end
         end
      end
   end
})

-- Player Tab
local PlayerTab = Window:CreateTab("Player", 17205450457)

PlayerTab:CreateButton({
   Name = "Toggle Invisible Script",
   Callback = function()
      toggles["Invisible Script"] = not toggles["Invisible Script"]
      if toggles["Invisible Script"] then
         loadstring(game:HttpGet('https://pastebin.com/raw/3Rnd9rHf'))()
      end
   end,
})

local currentSpeed = 16
PlayerTab:CreateSlider({
   Name = "Speed Slider",
   Range = {0, 100},
   Increment = 1,
   Suffix = "Speed",
   CurrentValue = currentSpeed,
   Flag = "Slider1",
   Callback = function(Value)
      currentSpeed = Value
   end,
})

-- Maintain speed persistently
task.spawn(function()
   while true do
      local player = game.Players.LocalPlayer
      if player and player.Character and player.Character:FindFirstChild("Humanoid") then
         player.Character.Humanoid.WalkSpeed = currentSpeed
      end
      task.wait(0.2)
   end
end)

-- TSB Tab
local TSBTab = Window:CreateTab("TSB", 17205450457)

TSBTab:CreateButton({
   Name = "Phantasm Hub",
   Callback = function()
      toggles["Phantasm Hub"] = not toggles["Phantasm Hub"]
      if toggles["Phantasm Hub"] then
         loadstring(game:HttpGet("https://raw.githubusercontent.com/ATrainz/Phantasm/main/Games/TSB.lua"))()
      end
   end,
})

TSBTab:CreateButton({
   Name = "Another TSB Script",
   Callback = function()
      toggles["Another TSB Script"] = not toggles["Another TSB Script"]
      if toggles["Another TSB Script"] then
         loadstring(game:HttpGet("https://raw.githubusercontent.com/ATrainz/Phantasm/main/Games/TSB.lua"))()
      end
   end,
})

-- Garden Tab
local GardenTab = Window:CreateTab("Grow a Garden", 17205450457)

GardenTab:CreateButton({
   Name = "Garden Script",
   Callback = function()
      toggles["Garden Script"] = not toggles["Garden Script"]
      if toggles["Garden Script"] then
         loadstring(game:HttpGet("https://raw.githubusercontent.com/tesghg/Grow-a-Garden/main/ameicaa_Grow_A_Garden.lua"))()
      end
   end,
})
